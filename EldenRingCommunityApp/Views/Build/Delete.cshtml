@model Build
@{
	ViewData["Title"] = "Delete Build Page";
}

<div class="text-center">
	<h1 class="display-4">Delete @(Model.Name)?</h1>
</div>

<div>
	<form asp-action="Delete" method="post">
		<input type="hidden" asp-for="BuildID" />
		<button type="submit" class="btn btn-primary">Delete</button>
	</form>
	<button class="btn btn-primary" asp-action="List">Cancel</button>
</div>